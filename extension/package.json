{
  "name": "mirakl-tipsa-orchestrator-extension",
  "version": "0.2.0",
  "description": "Chrome extension for orchestrating Mirakl orders to TIPSA carrier",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist .vite .turbo coverage artifacts out",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "package:zip": "node scripts/zip-dist.mjs",
    "verify:sentinels": "node scripts/verify-sentinels.mjs",
    "prebuild": "npm run clean",
    "setup:env": "powershell -ExecutionPolicy Bypass -File scripts/setup-frontend-env.ps1",
    "activate:env": "powershell -ExecutionPolicy Bypass -File scripts/activate-frontend-env.ps1",
    "dev:env": "frontend-env\\Scripts\\activate && npm run dev",
    "build:env": "frontend-env\\Scripts\\activate && npm run build",
    "test:env": "frontend-env\\Scripts\\activate && npm run test:unit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.0",
    "zod": "^3.22.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/chrome": "^0.0.250",
    "@vitejs/plugin-react": "^4.2.0",
    "@crxjs/vite-plugin": "^2.0.0-beta.21",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-import": "^2.29.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "cross-env": "^7.0.3",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jsdom": "^23.0.0",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "sinon-chrome": "^3.0.1",
    "archiver": "^6.0.0",
    "crypto": "^1.0.1"
  }
}